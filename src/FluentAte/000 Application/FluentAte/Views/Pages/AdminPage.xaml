<UserControl x:Class="FluentAte.Views.Pages.AdminPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FluentAte.Views.Pages"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <Grid Margin="10" VerticalAlignment="Stretch">
        <ui:NavigationView x:Name="NavigationView"
                           MenuItemsSource="{Binding ViewModel.SubMenus}"
                           PaneDisplayMode="Top"
                           IsPaneToggleVisible="False"
                           FontSize="12"
                           IsBackButtonVisible="Collapsed" />
    </Grid>
</UserControl>
